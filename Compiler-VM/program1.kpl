Program program1;
   Var a: Integer;
   Var b: Integer;
   Var sum: Integer;
   Var diff: Integer;
   Var prod: Integer;

Begin
   a := 10;
   b := 4;

   sum  := a + b;
   diff := a - b;
   prod := a * b;

   Call WriteI(sum);
   Call WriteLn;
   
   Call WriteI(diff);
   Call WriteLn;
   
   Call WriteI(prod);
   Call WriteLn;
End.